{
  "domains": {
    "retapcard.com": {
      "type": "website",
      "path": "apps/website",
      "framework": "nextjs"
    },
    "app.retapcard.com": {
      "type": "flutter-web",
      "path": "apps/flutter-web-client",
      "framework": "static"
    }
  },
  "redirects": [
    {
      "source": "https://www.retapcard.com",
      "destination": "https://retapcard.com",
      "permanent": true
    },
    {
      "source": "https://www.app.retapcard.com",
      "destination": "https://app.retapcard.com",
      "permanent": true
    }
  ]
} 