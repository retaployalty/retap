{
  "version": 2,
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/build/web/index.html" }
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://retapcard.com",
    "NEXT_PUBLIC_FLUTTER_APP_URL": "https://app.retapcard.com"
  }
} 